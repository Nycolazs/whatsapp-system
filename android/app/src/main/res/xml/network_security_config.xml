<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Release local/LAN: permite HTTP para backend por IP na rede interna -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>
