{
  "name": "whatsapp-system",
  "version": "1.0.0",
  "description": "Sistema WhatsApp",
  "main": "backend/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run start:backend || true; npm run start:frontend || true",
    "start:backend": "SERVE_FRONTEND=0 ./start start",
    "start:frontend": "./start-frontend start",
    "start:menu": "./start",
    "start:direct": "SERVE_FRONTEND=0 node backend/index.js",
    "stop": "npm run stop:frontend || true; npm run stop:backend || true",
    "stop:backend": "./start stop",
    "stop:frontend": "./start-frontend stop",
    "restart": "npm run restart:backend || true; npm run restart:frontend || true",
    "restart:backend": "./start restart",
    "restart:frontend": "./start-frontend restart",
    "status": "npm run status:backend && npm run status:frontend",
    "status:backend": "./start status",
    "status:frontend": "./start-frontend status",
    "logs": "./start tail",
    "logs:frontend": "./start-frontend tail",
    "logs:clear": "./start logs",
    "logs:clear:frontend": "./start-frontend logs",
    "reset": "./start reset",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "multer": "^2.0.2"
  }
}
